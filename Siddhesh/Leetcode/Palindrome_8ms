//This is no an ideal way to solve this
class Solution {
public:
    bool isPalindrome(int x) {
        string s=to_string(x);
        for (int i=0; i<s.size()/2; ++i){
          if  (s[i]==s[s.size()-1-i]){
            continue;
          }
            else{
                return false;
            }
        }
        return true;
    }
};
